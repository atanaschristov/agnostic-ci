{
	"name": "agnostic-ci",
	"version": "0.0.1-1",
	"description": "Agnostic-CI(ACI) is a library for building and processing command interfaces",
	"main": "es2020/index.js",
	"module": "es2020/index.mjs",
	"types": "index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./es2020/index.mjs",
			"require": "./es2020/index.js"
		},
		"./es5": {
			"types": "./index.d.ts",
			"require": "./es5/index.js"
		}
	},
	"files": [
		"es2020/**/*",
		"es5/**/*",
		"index.d.ts",
		"README.md",
		"LICENSE",
		"package.json"
	],
	"author": "dev <xfoocom@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/atanaschristov/agnostic-ci"
	},
	"keywords": [
		"web",
		"browser",
		"library",
		"node",
		"util",
		"cli",
		"builder"
	],
	"scripts": {
		"prestart": "cp build/pre-commit .git/hooks/ && chmod +x .git/hooks/pre-commit && echo 'hook copied'",
		"prebuild": "node ./scripts/getFiles.js",
		"clean:dist": "rimraf ./dist",
		"clean:meta": "rimraf '**/.DS_Store' '**/__MACOSX' '**/.Spotlight-V100' '**/.Trashes' '**/.fseventsd' '**/Thumbs.db' '**/ehthumbs.db' '**/desktop.ini'",
		"clean": "npm run clean:dist",
		"prepackage": "npx cpy-cli package.json README.md LICENSE dist/lib/",
		"typecheck": "tsc --noEmit -p tsconfig.lib.json",
		"compile:types": "npx dts-bundle-generator -o dist/lib/index.d.ts src/lib/index.ts --project tsconfig.dts.json --external-inlines @babel/types",
		"compile": "npm run clean && npm run typecheck && npm run compile:types && npx tsup && npm run prepackage",
		"compile:cli": "rimraf ./dist/demo/cli && npx tsc --build src/demo/cli/tsconfig.demo.json",
		"run:cli:lib": "npm run compile && npm run compile:cli && node src/demo/cli/index.cjs",
		"run:cli:ts": "npx ts-node -P src/demo/cli/tsconfig.demo.json src/demo/cli/index.ts",
		"run:cli": "npm run run:cli:lib",
		"run:web": "cd src/demo/web && npm run dev",
		"build:web": "cd src/demo/web && set NODE_ENV='production' && npm run build",
		"test": "npm run clean && npx jest --config jest.config.js",
		"test:watch": "npm run clean && npx jest --config jest.config.js --watch",
		"test:coverage": "npm run clean && npx jest --config jest.config.js --coverage"
	},
	"devDependencies": {
		"@eslint/css": "^0.11.0",
		"@eslint/js": "^9.36.0",
		"@eslint/json": "^0.13.2",
		"@swc/core": "^1.13.5",
		"@types/jest": "^29.5.14",
		"@types/node": "^24.1.0",
		"@types/object-hash": "^3.0.6",
		"bem-cn": "^3.0.1",
		"clsx": "^2.1.1",
		"cpy-cli": "^6.0.0",
		"dts-bundle-generator": "^9.5.1",
		"eslint": "^9.36.0",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.4.0",
		"jest": "^29.7.0",
		"jest-transform-stub": "^2.0.0",
		"jiti": "^2.6.0",
		"nodemon": "^3.1.10",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.3.2",
		"tsup": "^8.5.0",
		"typescript": "^5.9.0",
		"typescript-eslint": "^8.44.0"
	},
	"dependencies": {
		"i18next": "^25.3.6",
		"object-hash": "^3.0.0"
	}
}
